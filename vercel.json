{
  "version": 2,
  "alias": ["disasterrecoveryqld.au", "www.disasterrecoveryqld.au"],
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://disasterrecoveryqld.au"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/en-AU",
      "permanent": true
    }
  ]
}
